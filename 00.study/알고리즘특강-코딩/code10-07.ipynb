   "source": [
    "# 구구단 호출"
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "source": [
    "def gugu(dan, num):\r\n",
    "    print(f\"{dan} x {num} = {dan * num}\")\r\n",
    "    if num < 9 :\r\n",
    "        gugu(dan, num+1)"
   ],
   "outputs": [],
   "metadata": {}
  },
